{
    "name": "ourchitecture",
    "private": true,
    "version": "0.0.1",
    "license": "ISC",
    "displayName": "Ourchitecture",
    "description": "Ourchitecture website",
    "homepage": "https://ourchitecture.io/",
    "author": "Ourchitecture Open Source",
    "repository": {
        "type": "git",
        "url": "https://github.com/ourchitecture/site-ionic-ng.git"
    },
    "contributors": [
        "Eric Swanson (@ericis)"
    ],
    "scripts": {
        "ng": "ng",
        "start": "npm run build:content:ci && ng serve",
        "start:prod": "npm run build:ci && http-server -p 4200 -c-1 www",
        "clean:content": "rimraf ./src/app/content/pages",
        "build:content": "npm run clean:content && nodemon --watch content --watch build -e \"md,ts\" ./build/content.js",
        "build:content:ci": "npm run clean:content && node ./build/content",
        "build": "npm run build:content && ng build",
        "build:ci": "ng build --prod && cp ./www/index.html ./www/404.html",
        "test": "npm run build:content && ng test",
        "test:ci": "ng test --prod=true",
        "lint": "ng lint",
        "lint:ci": "ng lint",
        "e2e": "npm run build:content && ng e2e --port=4201",
        "e2e:ci": "ng e2e --prod=true"
    },
    "dependencies": {
        "@angular/animations": "^8.2.14",
        "@angular/cdk": "~8.2.3",
        "@angular/common": "^8.2.14",
        "@angular/core": "^8.2.14",
        "@angular/forms": "^8.2.14",
        "@angular/material": "^8.2.3",
        "@angular/platform-browser": "^8.2.14",
        "@angular/platform-browser-dynamic": "^8.2.14",
        "@angular/pwa": "^0.12.4",
        "@angular/router": "^8.2.14",
        "@angular/service-worker": "^8.2.14",
        "@covalent/core": "^2.0.2",
        "@covalent/markdown": "^2.1.1",
        "@ionic-native/core": "^5.0.0",
        "@ionic-native/splash-screen": "^5.0.0",
        "@ionic-native/status-bar": "^5.0.0",
        "@ionic/angular": "^4.1.0",
        "core-js": "^2.5.4",
        "hammerjs": "^2.0.8",
        "ionic": "^5.4.15",
        "rxjs": "~6.5.4",
        "tslib": "^1.9.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.803.23",
        "@angular-devkit/build-angular": "^0.803.23",
        "@angular-devkit/core": "~8.3.23",
        "@angular-devkit/schematics": "~8.3.23",
        "@angular/cli": "~8.3.23",
        "@angular/compiler": "~8.2.14",
        "@angular/compiler-cli": "~8.2.14",
        "@angular/language-service": "~8.2.14",
        "@ionic/angular-toolkit": "~1.5.1",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~12.0.0",
        "codelyzer": "~4.5.0",
        "fs-extra": "^8.1.0",
        "http-server": "^0.11.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "js-yaml": "^3.13.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "nodemon": "^1.19.1",
        "npm-run-all": "^4.1.5",
        "protractor": "~5.4.2",
        "puppeteer": "~1.15.0",
        "rimraf": "^2.6.3",
        "showdown": "^1.9.1",
